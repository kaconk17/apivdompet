{
  "name": "vdompet",
  "version": "1.0.0",
  "description": "rest API for vdompet android app",
  "main": "index.js",
  "scripts": {
    "create-dev-tables": "babel-node ./app/db/dev/dbConnection createAllTable",
    "start": "nodemon --watch . --exec babel-node -- server",
    "setup": "npm-run-all -p start create-dev-tables",
    "migrate_down": "db-migrate down -c 1 --config ./config/test_config.json",
    "migrate_up": "db-migrate up -c 1 --config ./config/test_config.json",
    "migrate_up_all": "db-migrate up --config ./config/test_config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaconk17/apivdompet.git"
  },
  "keywords": [
    "nodejs",
    "restapi"
  ],
  "author": "kaconk17",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kaconk17/apivdompet/issues"
  },
  "homepage": "https://github.com/kaconk17/apivdompet#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "babel-node": "0.0.1-security",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.8",
    "moment": "^2.29.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^7.0.0"
  }
}
